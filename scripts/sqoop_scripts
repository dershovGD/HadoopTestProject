sqoop export --connect jdbc:mysql://10.0.0.21:3306/dershov --table top_ten_products_for_each_category \
--hcatalog-database dershov --hcatalog-table top_ten_products_for_each_category --input-fields-terminated-by ',' \
--input-lines-terminated-by '\n'

sqoop export --connect jdbc:mysql://10.0.0.21:3306/dershov --table most_purchased_categories \
--hcatalog-database dershov --hcatalog-table most_purchased_categories --input-fields-terminated-by ',' \
--input-lines-terminated-by '\n'

sqoop export --connect jdbc:mysql://10.0.0.21:3306/dershov --table top_spending_countries \
--hcatalog-database dershov --hcatalog-table top_spending_countries --input-fields-terminated-by ',' \
--input-lines-terminated-by '\n'